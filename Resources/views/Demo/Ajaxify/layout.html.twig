{% extends 'ImaticViewBundle:Demo:base.html.twig' %}

{% block headline 'Ajaxify' %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'bundles/imaticview/ts/ajaxify/init.ts'
    output='js/ajaxify.js' %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    'bundles/imaticview/css/demo/ajaxify/ajaxify.css'
    output='css/ajaxify.css' -%}
    <link rel="stylesheet" href="{{ asset_url }}" media="screen" type="text/css">
    {% endstylesheets %}
{% endblock %}
