{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        $(function () {
            var $demoNav = $('#demo-nav');
            var $demoContent = $('#demo-content');
            var $ul = $('<ul>', { class: 'nav nav-list' });
            var i = 0;

            $('.menu', $demoContent).each(function () {
                var $this = $(this);
                var id = 'id-' + ++i;
                $this.attr('id', id);
                $ul.append($('<li>', { class: $this.is('h2') ? 'nav-header' : '', html: $('<a>', { text: $this.text(), href: '#' + id })}));
            });

            $demoNav.append($ul);
        });
    </script>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        h2.menu {
            border-bottom: 3px solid #ddd;
        }

        h3.menu {
            border-bottom: 1px solid #ddd;
        }
    </style>
{% endblock %}