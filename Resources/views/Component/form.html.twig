{#
    variables:
    - form
#}

{% block form %}
    {% if form_theme is defined %}
        {% form_theme form form_theme %}
    {% endif %}

    {{ form_javascript(form) }}
    {{ form_stylesheet(form) }}

    <div class="imatic-view-form">
        {{ form(form, {attr: {novalidate: 'novalidate' } }) }}
        {{ form_javascript(form) }}
    </div>
{% endblock %}
